
<% console.log(estimatePartLabors.query[0].entries); %>
<div class="container">
 <h2>All entries</h2>

<p>There are  <%= estimatePartLabors.query[0].entries.length %> entries. <br/>
<a href="/estimatePartLabor/create" class="btn btn-success"><i class="glyphicon glyphicon-pencil"></i>Add new one.</a></p>

<table class="table" border="1">
    <thead>
      <tr>
        <th>id</th>       
       <th>type</th>
       <th>count</th>
       <th>hoursPerPerson </th>
       <th>dollarsPerHour</th>
       <th>nightsPerPerson</th>
       <th>costPerNight</th>
       <th>Labour Cost</th>
        <th>Actions</th>
    </tr>
    </thead>
    <tbody>
<% for (var i = 0; i < estimatePartLabors.query.length; i++) { 
    var tot=0;
     for(var j=0; j<estimatePartLabors.query[i].entries.length;j++){
        var entry = estimatePartLabors.query[i].entries[j];
        var doll= entry.count*entry.hoursPerPerson*entry.dollarsPerHour + entry.nightsPerPerson*entry.costPerNight;
           
      %>
        <tr>
            <td> <%= entry._id %></td>
            <td> <%= entry.type %></td>
            <td> <%= entry.count %></td>
            <td> <%= entry.hoursPerPerson %>
            <td> <%= entry.dollarsPerHour %></td>
            <td> <%= entry.nightsPerPerson %></td>
            <td> <%= entry.costPerNight %></td></td>
            <td bgcolor = "#ffffff"> <%= doll %></td>
            <%tot+= doll %>

             
            <td>
            <a href="estimatePartLabor/delete/<%=  entry._id %>" class="btn btn-danger"><i class="glyphicon glyphicon-trash"></i> Delete </a> 
							<a href="estimatePartLabor/details/<%= entry._id %>" class="btn btn-info"><i class="glyphicon glyphicon-list-alt"></i> Details </a> 
							<a href="estimatePartLabor/edit/<%=   entry._id %>" class="btn btn-warning"><i class="glyphicon glyphicon-edit"></i> Edit </a>
            </td>
         </tr>
         <% } %>
         <% } %>
        
         <tr> 
            <td colspan="9" bgcolor = "#fff"> <strong> Calculated Labor:  <%=  tot+"$" %> </strong></span> </td>
            
         </tr>


</tbody>
</table>
</div>
