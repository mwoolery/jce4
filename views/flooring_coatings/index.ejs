            <a  target="blank" href="/flooring_coatings/details"  <%= flooringCoatings.query[i].id %>"> Details</a> |
            <a target="blank" href="/flooring_coatings/edit" <%= flooringCoatings.query[i].id %>">  Edit </a> 
            
            </td>
         </tr>
         <% } %>
</tbody>
</table>
</div>

