<h2>Edit waterproofing primer product</h2>
<div class="content">
	<div>
		<form method="POST" action="/estimatePartWaterproofing/save/<%= estimatePartWaterproofing._id %>">

			<div class="form-group" id=frm>
				<label for="_id">ID</label>
				<input type="number" class="form-control" id="_id" name="_id" value="<%= estimatePartWaterproofing._id %>" required disabled>
			</div>

			<div class="form-group">
				<label for="name">Product Type</label>
				<input type="text" class="form-control" id="productType" name="productType" value="<%= estimatePartWaterproofing.productType %>" required>
			</div>

			<div class="form-group">
				<label for="unit">UsesUrethane</label>
				<input type="text" class="form-control" id="usesUrethane" name="usesUrethane" value="<%= estimatePartWaterproofing.usesUrethane %>" required>
			</div>

			<div class="form-group">
				<label for="price">Sub Total</label>
				<input type="number" class="form-control" id="subtotal" name="subtotal" value="<%= estimatePartWaterproofing.subtotal %>" required>
			</div>

			<input type="submit" value="Save" class="btn btn-primary" />
		</form>
		<br/>
		<form method="get" action="/estimatePartWaterproofing">
			<input type="submit" value="Return to list" class="btn btn-caution" />
		</form>
	</div>
</div>

