<h2>Edit Aggregate Cost</h2>
<div class="content">
	<div>
		<form method="POST" action="/estimatePartAggregate/save/<%= estimatePartAggregate._id %>">

			<div class="form-group" id=frm>
				<label for="_id">ID</label>
				<input type="number" class="form-control" id="_id" name="_id" value="<%= estimatePartAggregate._id %>" required disabled>
			</div>

			<div class="form-group">
				<label for="isUsed">isUsed</label>
				<input type="text" class="form-control" id="isUsed" name="isUsed" value="<%= estimatePartAggregate.isUsed %>" required>
			</div>

			<div class="form-group">
				<label for="aggregateTypeSelection">Type</label>
				<input type="text" class="form-control" id="aggregateTypeSelection" name="aggregateTypeSelection" value="<%= estimatePartAggregate.aggregateTypeSelection %>" required>
			</div>

			<div class="form-group">
				<label for="aggregateMaterialSelection">Selection</label>
				<input type="number" class="form-control" id="aggregateMaterialSelection" name="aggregateMaterialSelection" value="<%= estimatePartAggregate.aggregateMaterialSelection %>" required>
			</div>

			<div class="form-group">
				<label for="coverageSqFt">Coverage</label>
				<input type="number" class="form-control" id="coverageSqFt" name="coverageSqFt" value="<%= estimatePartAggregate.coverageSqFt %>"
					required>
			</div>
			<div class="form-group">
				<label for="subtotal">Subtotal</label>
				<input type="number" class="form-control" id="subtotal" name="subtotal" value="<%= estimatePartAggregate.subtotal %>"
					required>
			</div>

			<input type="submit" value="Save" class="btn btn-primary" />
		</form>
		<br/>
		<form method="get" action="/estimatePartAggregate">
			<input type="submit" value="Return to list" class="btn btn-caution" />
		</form>
	</div>
</div>

