<h2>Edit waterproofing primer product ***</h2>
<div class="content">
	<div>
		<form method="POST" action="/estimatePartAggregate/save/<%= estimatePartAggregate._id %>">

			<div class="form-group" id=frm>
				<label for="_id">ID</label>
				<input type="number" class="form-control" id="_id" name="_id" value="<%= estimatePartAggregate._id %>" required disabled>
			</div>

			<div class="form-group">
				<label for="name">isused</label>
				<input type="text" class="form-control" id="name" name="name" value="<%= estimatePartAggregate.isUsed %>" required>
			</div>

			<div class="form-group">
				<label for="unit">Aggregate Type Selection</label>
				<input type="text" class="form-control" id="unit" name="unit" value="<%= estimatePartAggregate.aggregateTypeSelection %>" required>
			</div>

			<div class="form-group">
				<label for="price">Aggregate Material Selection</label>
				<input type="number" class="form-control" id="price" name="price" value="<%= estimatePartAggregate.aggregateMaterialSelection %>" required>
			</div>

			<div class="form-group">
				<label for="displayorder">Coverage Sq. Ft.</label>
				<input type="number" class="form-control" id="displayorder" name="displayorder" value="<%= estimatePartAggregate.coverageSqFt %>"
					required>
			</div>
			<div class="form-group">
				<label for="displayorder">Sub Total</label>
				<input type="number" class="form-control" id="displayorder" name="displayorder" value="<%= estimatePartAggregate.subtotal %>"
					required>
			</div>

			<input type="submit" value="Save" class="btn btn-primary" />
		</form>
		<br/>
		<form method="get" action="/estimatePartAggregate">
			<input type="submit" value="Return to list" class="btn btn-caution" />
		</form>
	</div>
</div>

