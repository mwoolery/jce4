
<% console.log(aggregateMaterials.query); %>
<div class="container">
 <h2>Aggregate material costs</h2>

<p>There are  <%= aggregateMaterials.query.length %> aggregates. <br/>
<a href="create.ejs">Add a new one.</a></p>

<table class="table" border="1">
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Display Order</th>
        <th>Price</th>
        <th>Unit</th>
       <th>Actions</th>
    </tr>
    </thead>
    <tbody>
<% for (var i = 0; i < aggregateMaterials.query.length; i++) { %>
         <tr>
            <td><%=  aggregateMaterials.query[i].id %></td>
            <td><%=  aggregateMaterials.query[i].name %></td>
            <td><%=  aggregateMaterials.query[i].displayorder %></td>
            <td><%=  aggregateMaterials.query[i].price %></td>
            <td><%=  aggregateMaterials.query[i].unit %></td>
            <td>
							<a href="estimatePartAggregate/delete/<%=  aggregateMaterials.query[i]._id %>"> Delete </a> |
							<a href="estimatePartAggregate/details/<%= aggregateMaterials.query[i]._id %>"> Details </a> |
							<a href="estimatePartAggregate/edit/<%=    aggregateMaterials.query[i]._id %>"> Edit </a>
						</td>
         </tr>
         <% } %>
</tbody>
</table>
</div>
