
<% console.log(estimatePartRoofings.query); %>
<div class="container">
 <h2>All entries</h2>

<p>There are  <%= estimatePartRoofings.query.length %> entries. <br/>
<a href="estimatePartRoofing/create">Add a new one.</a></p>

<table class="table" border="1">
    <thead>
      <tr>
        <th>ID</th>
        <th>Roof Type</th>
        <th>Process Type</th>
        <th>Subtotal</th>
       <th>Actions</th>
    </tr>
    </thead>
    <tbody>
    <% count=0; %>
<% for (var i = 0; i < estimatePartRoofings.query.length; i++) { %>
         <tr><% count ++; %>
            
            <td><%=  estimatePartRoofings.query[i]._id %></td>
            <td><%=  estimatePartRoofings.query[i].roofType %></td>
            <td><%=  estimatePartRoofings.query[i].processType %></td>
            <td><%=  estimatePartRoofings.query[i].subtotal %></td>
             <td>
							<a href="estimatePartRoofing/delete/<%=  count%><button type="button" class="btn btn-danger"> <span class="glyphicon glyphicon-trash"></span> Delete </a> 
							<a href="estimatePartRoofing/details/<%=  count%><button type="button" class="btn btn-primary"> <span class="glyphicon glyphicon-edit"></span> Details </a> 
							<a href="estimatePartRoofing/edit/<%=  count%><button type="button" class="btn btn-warning"> <span class="glyphicon glyphicon-pencil"></span> Edit </a>
						</td>
        
        
        
         </tr>
         <% } %>
</tbody>
</table>
</div>
